<% content_for :header do %>
<div id="header">
		<h1 class="blog">
			<a href="http://chrissutton.me/">Chris Sutton</a> <span class="sub">presents</span> @<%= @filtered_data.first[1] %>
		</h1>
		<h3> The freshest links from <%= @filtered_data.first[2] %>, all in one place.</h3>
	</div>
<% end %>

<% @filtered_data.each do |a| %>
<div class="col300 article">
	<p><%= a[0] %></p>
<p class="timestamp"><%= a[3] %> Retweets</p>
</div>
<% end %>
